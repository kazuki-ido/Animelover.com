<%= stylesheet_link_tag 'views/users', media: 'all', 'data-turbolinks-track': 'reload' %>

<h1>ユーザー一覧</h1>

<ul class= "users">
  <% @users.each do |user| %>
    <li>
      <%= image_tag user.image.url, class: "icon_image_list", onerror: 'this.onerror=null;this.src="/images/user/image/s0041_30_0.jpg"' %>
      <%= link_to user.name, user %>
    </li>
  <% end %>
</ul>

<%= paginate @users%>